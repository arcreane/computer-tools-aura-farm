from save_manager import SaveManager
save_manager = SaveManager()

game_data = {                     #Données du jeu?
    "score": 100,     #exemples de données de jeu
    "level": 2,
    "position": {"x","y"},
    "ship_upgrades": ["shield", "..."] }

save_manager.save_game("save1.json", game_data) #Sauvegarder les données

print("Sauvegardes disponibles :", save_manager.list_saves()) #Lister les sauvegardes
